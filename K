local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/RedzLibV5/main/Source.Lua"))()
local Window = redzlib:MakeWindow({
    Title = "GLP Client | Troll EB",
    SubTitle = "Powered_By_GLP_Team",
    SaveFolder = "GLPClientCfg.lua"
  })

  local Tab1 = Window:MakeTab({"Troll EB", "cherry"})
  local Tab2 = Window:MakeTab({"JJ'S", "swords"})
  local Tab3 = Window:MakeTab({"Ui", "user"})

