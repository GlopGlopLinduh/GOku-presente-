    redzlib:SetTheme("Dark")
  end})
  Tab3:AddButton({"Darker Theme", function()
    redzlib:SetTheme("Darker")
  end})

  local Section = Tab1:AddSection({"EB Goku"})

  local Button = Tab2:AddButton({"Click Explode",
  Callback = function ()
